                        }
                    },
                    "202": {
                        "description": "ACCEPTED -- Operation accepted and will be performed asynchronously",
                        "schema": {
                            "description": "location migrated too",
                            "type": "string"
                        }
                    }
                },
                "x-ms-long-running-operation": true
            }
